<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" version="2.5"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    
    <servlet>
        <servlet-name>RegistrationHandlerServlet</servlet-name>
        <servlet-class>ru.nlmk_it.edu.RegistrationHandlerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AllUsersServlet</servlet-name>
        <servlet-class>ru.nlmk_it.edu.AllUsersServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>RegistrationHandlerServlet</servlet-name>
        <url-pattern>/rhs</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AllUsersServlet</servlet-name>
        <url-pattern>/allUsers</url-pattern>
    </servlet-mapping>
    
</web-app>